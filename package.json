{
  "name": "irango-api",
  "version": "0.0.1",
  "description": "",
  "author": "Matob <matob@live.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && nest build",
    "start": "node dist/infra/web/nestjs/main.js",
    "start:dev": "nest start --watch --preserveWatchOutput --entryFile=/infra/web/nestjs/main.js",
    "start:debug": "nest start --debug --watch --entryFile=/infra/web/nestjs/main.js",
    "start:prod": "node dist/infra/web/nestjs/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/typeorm/TypeOrmMigrationConfig.ts migration:generate",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/typeorm/TypeOrmMigrationConfig.ts migration:run",
    "migration:run:test": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource test/integration/setup/database/TypeOrmTestMigrationConfig.ts migration:run",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/typeorm/TypeOrmMigrationConfig.ts migration:revert",
    "seed:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/typeorm/TypeOrmSeedConfig.ts migration:run",
    "seed:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/config/typeorm/TypeOrmSeedConfig.ts migration:revert",
    "test:integration:one": "jest $1 --config jest.config.integration.js --runInBand --logHeapUsage --detectOpenHandles",
    "test:integration": "jest test/integration/features --config jest.config.integration.js --runInBand --logHeapUsage",
    "test": "npm run test:integration"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^10.0.1",
    "@sentry/node": "^7.88.0",
    "@sentry/profiling-node": "^1.3.2",
    "cache-manager": "^5.3.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "mysql2": "^3.6.5",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-angular": "^18.4.3",
    "@faker-js/faker": "^8.3.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@stylistic/eslint-plugin": "^1.5.1",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "leaked-handles": "^5.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
